connector.class=com.caleb.kafka.connect.jdbc.QueryBasedJdbcSinkConnector
jdbc.password=password
jdbc.username=root
jdbc.connection=jdbc:mysql://172.17.0.1:3307/gdb?autoReconnect=true&serverTimezone=America%2FDenver
